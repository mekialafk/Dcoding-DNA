	var x;
		function runGame(){
				timer.start(1000);
				colorBoard();
				var x = Math.floor(Math.random()*4);
				timer.start(1000);
			}
		function colorBoard(){
			console.log(x);
			for(var j = 0; j<33; j++){
				var boardID = "r"+0+"c"+j;
				x = Math.floor(Math.random()*4);
				console.log(boardID);
				if(x==0){
					document.getElementById(boardID).style.background = "green";
				}
				if(x==1){
					document.getElementById(boardID).style.background = "yellow";
				}
				if(x==2){
					document.getElementById(boardID).style.background = "blue";
				}
				if(x==3){
					document.getElementById(boardID).style.background = "red";
				}
			}
	}
