		var j = 0;
		function keyPress(e){
				var boardID = "r"+0+"c"+j;
				if (e.keyCode==65 && document.getElementById(boardID).style.background == "green"){
					document.getElementById(boardID).style.background = "lightsteelblue";
					console.log("yes");
					j=j+1;
					boardID = "r"+0+"c"+j;
					console.log(boardID);
				}
				else if (e.keyCode==65 && document.getElementById(boardID).style.background !== "green"){
					console.log("no");
					alert("No,try again!");
					j=j+1
				}
				else if (e.keyCode==67 && document.getElementById(boardID).style.background == "red"){
					document.getElementById(boardID).style.background = "lightsteelblue";
					console.log("yes");
					j=j+1;
					boardID = "r"+0+"c"+j;
					console.log(boardID);
				}
				else if (e.keyCode==67 && document.getElementById(boardID).style.background !== "red"){
					console.log("no");
					alert("No,try again!");
					j=j+1
				}
				else if (e.keyCode==71 && document.getElementById(boardID).style.background == "yellow"){
					document.getElementById(boardID).style.background = "lightsteelblue";
					console.log("yes");
					j=j+1;
					boardID = "r"+0+"c"+j;
					console.log(boardID);
				}
				else if (e.keyCode==71 && document.getElementById(boardID).style.background !== "yellow"){
					console.log("no");
					alert("No,try again!");
					j=j+1
				}
				else if (e.keyCode==84 && document.getElementById(boardID).style.background == "blue"){
					document.getElementById(boardID).style.background = "lightsteelblue";
					console.log("yes");
					j=j+1;
					boardID = "r"+0+"c"+j;
					console.log(boardID);
				}
				else if (e.keyCode==84 && document.getElementById(boardID).style.background !== "blue"){
					console.log("no");
					alert("No,try again!");
					j=j+1;
				}
				else if (e.keyCode==8){
					j=j-1;
					boardID = "r"+0+"c"+j;
				}
			if(j==33){
				alert("Great Job! Time to Move Onto Transcription!");
				transcriptiontime2();
				this.stop();
				var j = 0;
			}
		}

		

